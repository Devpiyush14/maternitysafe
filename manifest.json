
  {
    "manifest_version": 3,
    "name": "MaternitySafe",
    "version": "1.0",
    "description": "MaternitySafe",
    "permissions": [
      "activeTab",
      "scripting",
      "storage"
    ],
    "host_permissions": [
      "https://api.openai.com/*"
    ],
    "action": {
      "default_popup": "popup.html"
    },
    "icons": {
      "48": "icon48.png",
      "128": "icon128.png"
    }
  }